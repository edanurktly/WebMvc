@model ICollection<Shipper>

<a class="btn btn-success" asp-action="Create" asp-controller="Shipper">Yeni Kayit Olustur</a>
<table class="table table-hover">
    <thead>
        <tr>
            <th>Id</th>
            <th>Firma Adi</th>
            <th>Telefon</th>
            <th></th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.ShipperId</td>
                <td>@item.CompanyName</td>
                <td>@item.Phone</td>
                <td>
                    <a class="btn btn-primary" asp-action="Update" asp-controller="Shipper" asp-route-id="@item.ShipperId">Guncelle</a>
                    <a class="btn btn-danger" asp-action="Delete" asp-controller="Shipper" asp-route-id="@item.ShipperId">Sil</a>
                </td>
            </tr>
        }
    </tbody>


</table>